{<% if (packageName) { %>
  "name": "<%= packageName %>",<% } %><% if (specVersion) { %>
  "version": "<%= specVersion %>",<% } %>
  "dependencies": {
    "bower": "^1.7.7",
    "cors": "^2.7.1",
    "deploy-to-gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-connect": "^4.2.0",
    "portfinder": "^1.0.3",
    "swagger-repo": "^1.0.0",<% if (installSwaggerUI) { %>
    "swagger-ui": "^2.1.4",<% } %>
    "shelljs": "^0.7.0"
  },
  "private": true,
  "scripts": {
    "deploy": "npm run build && deploy-to-gh-pages <% if (branchPreview) { %>--update <% } %>web_deploy",
    "build": "node ./scripts/build.js",
    "swagger": "swagger-repo",
    "test": "swagger-repo validate",
    "start": "gulp serve"<% if (branchPreview) { %>,
    "deploy-branch": "node ./scripts/deploy-branch"<% } %>
  }
}
